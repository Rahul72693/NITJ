#include<stdio.h>
int main()
{
	int a,b,c;
	printf("Enter three numbers");
	scanf("%d%d%d",&a,&b,&c);	
	if(a>=b&&a>=c)
	printf("%d is the largest of the three numbers",a);
	if(b>=a&&b>=c)
	printf("%d is the largest number of the three",b);
	if(c>=a&&c>=b)
	printf("%d is the largest number of the three",c);
	return 0;
}
